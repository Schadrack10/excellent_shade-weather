{
  "expo": {
    "name": "excellent_shade Weather-App",
    "slug": "excellent-weather-app",
    "description":"this is a weather app created by schadrack Botombe ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand.png",
    "splash": {
      "image": "./assets/weather-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
